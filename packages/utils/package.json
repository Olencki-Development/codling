{
  "name": "@codling/utils",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "compile": "tsc --build",
    "prepack": "npm run dist",
    "test": "mocha -r ts-node/register --project tsconfig.json \"./src/setup.test.ts\" \"./src/**/*.test.ts\"",
    "test:coverage": "nyc npm run test",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@codling/testable": "^1.0.0"
  },
  "dependencies": {
    "zod": "^3.19.1"
  }
}
